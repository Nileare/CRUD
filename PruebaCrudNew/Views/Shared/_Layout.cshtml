<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.8.3.js"></script>
</head>
<body>
    <div class="navbar navbar-expand-xl navbar-light bg-light">
        <div class="container">
            <div class="navbar-header">
                @Html.ActionLink("Estudiante", "Index", "Estudiante", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Universidad", "Index", "Universidad", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("About", "Index", "About", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                </ul>
            </div>
        </div>
    </div>
    @*footer*@
    <div class="container body-content  ">
        @RenderBody()
        <div class="card-header ">
            Nilearemc
        </div>
        <div class="card-body" >
            @*<h5 class="card-title ">Creado por Nile Are</h5>*@
            <p class="card-text">&copy; @DateTime.Now.Year - Crud en My ASP.NET Application</p>
            @*<a href="#" class="btn btn-primary">Go somewhere</a>*@
        </div>
    </div>
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.js" integrity="sha512-n/4gHW3atM3QqRcbCn6ewmpxcLAHGaDjpEBu4xZd47N0W2oQ+6q7oc3PXstrJYXcbNU1OHdQ1T7pAP+gi5Yu8g==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.3/font/bootstrap-icons.css">
</body>
</html>



























